    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/alteration"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geochemistry"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geology"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geophysics"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/metamorphic%20rocks"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/alteration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">alteration</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Local changes in the mineralogy of a rock body due to low-grade metamorphism, weathering, or both.  May indicate underlying mineral deposits.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/metamorphism%20%28geological%29"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/sediment%20transport">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">sediment transport</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Transport of solid particles of unconsolidated rock and mineral fragments, chemical precipitates, or biological materials.</skos:scopeNote>
    <skos:altLabel xml:lang="en">alluvial transport</skos:altLabel>
    <skos:altLabel xml:lang="en">bed load</skos:altLabel>
    <skos:altLabel xml:lang="en">bedload</skos:altLabel>
    <skos:altLabel xml:lang="en">density flow</skos:altLabel>
    <skos:altLabel xml:lang="en">dust transport</skos:altLabel>
    <skos:altLabel xml:lang="en">suspended load</skos:altLabel>
    <skos:altLabel xml:lang="en">turbidity flow</skos:altLabel>
