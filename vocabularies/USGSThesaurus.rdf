    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/suspended%20material%20%28water%29">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">suspended material (water)</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Sediment or particles of rock, sand, soil, and organic detritus carried in suspension in the water column, in contrast to sediment that moves on or near the streambed.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/water%20properties"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/hydrology"/>
