    <skos:prefLabel xml:lang="en">mining and quarrying</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Extracting mineral deposits from a pit, tunnel, or excavation.</skos:scopeNote>
    <skos:altLabel xml:lang="en">quarrying</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20extraction"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/placer%20deposit%20mining">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">placer deposit mining</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Extraction of minerals, typically metal ores, from unconsolidated surficial materials such as stream sediments.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20extraction"/>
