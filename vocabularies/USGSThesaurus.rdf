  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/glaciation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">glaciation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The geologic processes involved in the formation, movement, changes, and effects of large ice masses (glaciers).</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geologic%20processes"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/foliation%20%28geologic%29"/>
