  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/erosion">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">erosion</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The process whereby materials of the earth's crust are loosened, dissolved, or worn away and simultaneously moved from one place to another.</skos:scopeNote>
    <!--FOR shoreline erosion USE erosion WITH coastal processes-->
    <skos:altLabel xml:lang="en">shoreline erosion</skos:altLabel>
    <!--FOR coastal erosion USE erosion WITH coastal processes-->
    <skos:altLabel xml:lang="en">coastal erosion</skos:altLabel>
    <!--FOR beach erosion USE erosion WITH coastal processes-->
    <skos:altLabel xml:lang="en">beach erosion</skos:altLabel>
    <!--FOR shoreline retreat USE erosion WITH coastal processes-->
    <skos:altLabel xml:lang="en">shoreline retreat</skos:altLabel>
    <!--FOR wind erosion USE erosion WITH wind-->
    <skos:altLabel xml:lang="en">wind erosion</skos:altLabel>
    <skos:altLabel xml:lang="en">dissolution</skos:altLabel>
