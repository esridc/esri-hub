  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/grassland%20ecosystems">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">grassland ecosystems</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Biological communities and habitats with ground cover of grasses and other herbaceous plants, but few trees.  Examples include prairies, meadows, and savannas.</skos:scopeNote>
    <skos:altLabel xml:lang="en">mixed grass ecosystems</skos:altLabel>
    <skos:altLabel xml:lang="en">plains ecosystems</skos:altLabel>
    <skos:altLabel xml:lang="en">prairie ecosystems</skos:altLabel>
    <skos:altLabel xml:lang="en">short grass ecosystems</skos:altLabel>
    <skos:altLabel xml:lang="en">tall grass ecosystems</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/terrestrial%20ecosystems"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/island%20ecosystems">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">island ecosystems</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Communities of living organisms on islands.</skos:scopeNote>
    <skos:altLabel xml:lang="en">insular ecology</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/terrestrial%20ecosystems"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/shrubland%20ecosystems">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">shrubland ecosystems</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Biological land communities and habitats with sparse, low-growing vegetation.</skos:scopeNote>
    <skos:altLabel xml:lang="en">scrubland ecosystems</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/terrestrial%20ecosystems"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/tundra%20ecosystems">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">tundra ecosystems</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Biological communities and habitats located in the treeless plains of Arctic regions where the subsoil is permanently frozen.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/terrestrial%20ecosystems"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/wetland%20ecosystems">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">wetland ecosystems</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Ecosystems whose soil is saturated for long periods seasonally or continuously, including marshes, swamps, and ephemeral ponds.  More detailed terms for wetlands can be selected from the FGDC Wetland Classification &lt;http://fgdc.gov/standards/status/sub3_4.html&gt;.</skos:scopeNote>
    <skos:altLabel xml:lang="en">marsh ecosystems</skos:altLabel>
    <skos:altLabel xml:lang="en">marshland ecosystems</skos:altLabel>
    <skos:altLabel xml:lang="en">riparian ecosystems</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecosystems"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/wetland%20functions"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/aquatic%20biology"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecology"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/terrestrial%20ecosystems"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/habitats">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">habitats</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Parts of the physical environment where plants and animals live. Use in combination with terms from organisms and organism groupings (non-taxonomic) to indicate the topic of a species or group of species habitat.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/population%20and%20community%20ecology"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecosystems"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/phenology">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">phenology</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Study of the response of organisms to seasonal or interannual changes in the environment.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/population%20and%20community%20ecology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/population%20dynamics">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">population dynamics</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Changes in the size, composition, or structure of aggregates of single or multiple species in a specific area over time.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/population%20and%20community%20ecology"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/algal%20blooms"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/biogeography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/symbiosis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">symbiosis</skos:prefLabel>
    <skos:scopeNote xml:lang="en">An intimate, prolonged association between two or more organisms of different species that may or may not benefit each member.</skos:scopeNote>
    <skos:altLabel xml:lang="en">commensalism</skos:altLabel>
    <skos:altLabel xml:lang="en">mutualism</skos:altLabel>
    <skos:altLabel xml:lang="en">parasitism</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/population%20and%20community%20ecology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/recreation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">recreation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Pursuit of leisure-time activities, typically out of doors.</skos:scopeNote>
    <skos:altLabel xml:lang="en">biking</skos:altLabel>
    <skos:altLabel xml:lang="en">bird watching</skos:altLabel>
    <skos:altLabel xml:lang="en">boating</skos:altLabel>
    <skos:altLabel xml:lang="en">camping</skos:altLabel>
    <skos:altLabel xml:lang="en">canoeing</skos:altLabel>
    <skos:altLabel xml:lang="en">caving</skos:altLabel>
    <skos:altLabel xml:lang="en">climbing</skos:altLabel>
    <skos:altLabel xml:lang="en">fishing (recreational)</skos:altLabel>
    <skos:altLabel xml:lang="en">hiking</skos:altLabel>
    <skos:altLabel xml:lang="en">hunting</skos:altLabel>
    <skos:altLabel xml:lang="en">kayaking</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/topics"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
