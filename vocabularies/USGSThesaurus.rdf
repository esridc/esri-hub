  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/beach%20nourishment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">beach nourishment</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Transfer of sand from offshore areas to resupply beach areas that have been eroded.</skos:scopeNote>
    <skos:altLabel xml:lang="en">beach replenishment</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20extraction"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/dredging"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/biological%20population%20management">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">biological population management</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Judicious means of controlling or directing numbers and life conditions of organisms living in a specific locality to achieve a selected goal.</skos:scopeNote>
    <!--FOR conservation genetics USE biological population management WITH genetic diversity-->
    <skos:altLabel xml:lang="en">conservation genetics</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/fishery%20management"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/pest%20control"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/reintroduction%20%28organisms%29"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/wildlife%20population%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecosystem%20monitoring"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/population%20and%20community%20ecology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/fishery%20management">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">fishery management</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Controlling or directing numbers and life conditions of fish living in a specific locality to achieve a selected goal.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/biological%20population%20management"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/pest%20control">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">pest control</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Intentional reduction in the abundance of specific harmful organisms in a given area.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/biological%20population%20management"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/reintroduction%20%28organisms%29">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">reintroduction (organisms)</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Return of a species to an area through the intervention of man.</skos:scopeNote>
    <skos:altLabel xml:lang="en">re-establishment (organisms)</skos:altLabel>
    <skos:altLabel xml:lang="en">recolonization (organisms)</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/biological%20population%20management"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/wildlife%20population%20management">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">wildlife population management</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Monitoring and control of wildlife as a sustainable natural asset.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/biological%20population%20management"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/game%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/biogeography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/wildlife%20biology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/game%20management">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">game management</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Scientific monitoring and control of hunted wildlife.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/wildlife%20population%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/wildlife%20biology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/controlled%20flooding">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">controlled flooding</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The deliberate inundation of land or wetland, or an increase in river flow below dams for restoration or research purposes. Do not confuse with flood control.</skos:scopeNote>
    <skos:altLabel xml:lang="en">flooding (controlled)</skos:altLabel>
    <skos:altLabel xml:lang="en">managed flooding</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/floods"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/ecosystem%20management">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">ecosystem management</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Means of controlling or directing human practices, species populations, and physical environment within an ecosystem to achieve a selected state of sustainability.</skos:scopeNote>
    <skos:altLabel xml:lang="en">reintroduction (organisms)</skos:altLabel>
    <skos:altLabel xml:lang="en">ecosystem preservation</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecosystem%20monitoring"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecosystem%20services%20valuation"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/fire%20control">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">fire control</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Human intervention to restrict or prevent fires in forests and grasslands.</skos:scopeNote>
    <skos:altLabel xml:lang="en">fire prevention</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/controlled%20fires"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/controlled%20fires"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/controlled%20fires">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">controlled fires</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Prescribed burns used to burn trees, brush, and undergrowth that would fuel a large wildfire.  Do not confuse this term with 'fire control'.  It is one of the tools used in fire control.</skos:scopeNote>
    <skos:altLabel xml:lang="en">controlled burns</skos:altLabel>
    <skos:altLabel xml:lang="en">fires (controlled)</skos:altLabel>
    <skos:altLabel xml:lang="en">managed fires</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/fire%20control"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/fires"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/fire%20control"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20assessment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">natural resource assessment</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Estimation of the actual or potential value of natural materials and processes.</skos:scopeNote>
    <skos:altLabel xml:lang="en">resource assessment</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/remediation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">remediation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Methods for decontaminating, reclaiming, and restoring natural resources or reducing the effects of hazards.</skos:scopeNote>
    <skos:altLabel xml:lang="en">reclamation</skos:altLabel>
    <skos:altLabel xml:lang="en">restoration</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/bioremediation"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecology"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecosystem%20monitoring"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/engineering%20geology"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/bioremediation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">bioremediation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Use of biological methods, usually involving microorganisms, to break down or neutralize contaminants in soil, water, and wastes.</skos:scopeNote>
    <skos:altLabel xml:lang="en">biological remediation</skos:altLabel>
    <skos:altLabel xml:lang="en">revegetation</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remediation"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecology"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ecosystem%20monitoring"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/water%20resource%20management">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">water resource management</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Methods of controlling or directing resources and activities related to water for a predetermined goal.</skos:scopeNote>
    <skos:altLabel xml:lang="en">artificial recharge</skos:altLabel>
    <skos:altLabel xml:lang="en">impoundment</skos:altLabel>
    <skos:altLabel xml:lang="en">reclaimed water</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/hydraulic%20engineering"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/watershed%20management">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">watershed management</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Controlling and directing the resources and activities associated with the land and streams of a drainage basin to maintain the supply and quality of water and prevent floods, erosions, and other destructive conditions.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20management"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/erosion"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/floods"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/streamflow"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/risk%20assessment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">risk assessment</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The evaluation of the likelihood of adverse effects due to a given factor such as energy development or recurring hazardous events such as earthquakes.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/management%20methods"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/earthquake%20probabilities"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/landslide%20susceptibility%20assessment"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/volcanic%20eruption%20prediction"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/earthquake%20probabilities">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">earthquake probabilities</skos:prefLabel>
    <skos:scopeNote xml:lang="en">That aspect of seismology that deals with the physical conditions or indications that precede an earthquake, in order to predict its probability, size, time, and location.</skos:scopeNote>
    <skos:altLabel xml:lang="en">earthquake forecasting</skos:altLabel>
    <skos:altLabel xml:lang="en">earthquake prediction</skos:altLabel>
    <!--FOR seismicity maps USE earthquake probabilities WITH maps and atlases-->
    <skos:altLabel xml:lang="en">seismicity maps</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/risk%20assessment"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/earthquakes"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/neotectonic%20processes"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/landslide%20susceptibility%20assessment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">landslide susceptibility assessment</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Estimation of the probability of occurrence and likely severity of landslides in a given area.</skos:scopeNote>
    <!--FOR landslide susceptibility maps USE landslide susceptibility assessment WITH maps and atlases-->
    <skos:altLabel xml:lang="en">landslide susceptibility maps</skos:altLabel>
    <skos:altLabel xml:lang="en">slope stability</skos:altLabel>
    <!--FOR slope stability maps USE landslide susceptibility assessment WITH maps and atlases-->
    <skos:altLabel xml:lang="en">slope stability maps</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/risk%20assessment"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/landslides"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/volcanic%20eruption%20prediction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">volcanic eruption prediction</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Estimation of future volcanic activity using geological and geophysical observations.</skos:scopeNote>
    <skos:altLabel xml:lang="en">volcanic eruption forecasting</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/risk%20assessment"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/volcanic%20activity"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/photography">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">photography</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Process of using digital or film cameras to collect images of objects.</skos:scopeNote>
    <!--FOR photographs USE photography WITH image collections-->
    <skos:altLabel xml:lang="en">photographs</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/methods"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/underwater%20photography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/videography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/aerial%20photography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/underwater%20photography">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">underwater photography</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Photographs taken below the water surface, usually in marine, lacustrine, and estuarine environments.  Subjects are typically benthic organisms and sedimentary structures on the sea floor.</skos:scopeNote>
    <skos:altLabel xml:lang="en">camera tows</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/photography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ocean%20sciences"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">remote sensing</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Acquiring information about a natural feature or phenomenon, such as the Earth's surface, without actually being in contact with it. USGS remote sensing is usually carried out with airborne or spaceborne sensors or cameras.</skos:scopeNote>
    <!--FOR remote-sensing data USE remote sensing WITH image collections-->
    <skos:altLabel xml:lang="en">remote-sensing data</skos:altLabel>
    <!--FOR remote-sensing images USE remote sensing WITH image collections-->
    <skos:altLabel xml:lang="en">remote-sensing images</skos:altLabel>
    <skos:altLabel xml:lang="en">airborne imaging</skos:altLabel>
    <!--FOR composite burn index USE remote sensing WITH fire damage-->
    <skos:altLabel xml:lang="en">composite burn index</skos:altLabel>
    <!--FOR normalized burn ratio USE remote sensing WITH fire damage-->
    <skos:altLabel xml:lang="en">normalized burn ratio</skos:altLabel>
    <skos:altLabel xml:lang="en">photogrammetry</skos:altLabel>
    <!--FOR satellite altimetry USE remote sensing WITH altimetry measurement-->
    <skos:altLabel xml:lang="en">satellite altimetry</skos:altLabel>
    <skos:altLabel xml:lang="en">satellite imaging</skos:altLabel>
    <skos:altLabel xml:lang="en">space-borne sensing</skos:altLabel>
    <!--FOR satellite images USE remote sensing WITH image collections-->
    <skos:altLabel xml:lang="en">satellite images</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/methods"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/IFSAR"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/LIDAR"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/aerial%20photography"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/aeromagnetic%20surveying"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/aeroradiometric%20surveying"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/hyperspectral%20imaging"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/infrared%20imaging"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/microwave%20imaging"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/multispectral%20imaging"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/panchromatic%20imaging"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/radar%20imaging"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/thermal%20imaging"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/visible%20light%20imaging"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/seismic%20methods"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/telemetry"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/videography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/IFSAR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">IFSAR</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Interferometric comparison of radar reflectance imagery of the same area at different times to determine changes in the land surface, typically from a space-borne sensor.</skos:scopeNote>
    <skos:altLabel xml:lang="en">Interferometric Synthetic Aperture Radar (IFSAR)</skos:altLabel>
    <skos:altLabel xml:lang="en">SAR interferometry</skos:altLabel>
    <skos:altLabel xml:lang="en">InSAR</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/LIDAR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">LIDAR</skos:prefLabel>
    <skos:scopeNote xml:lang="en">LIDAR (LIght Detection and Ranging) is an airborne, spaceborne or ground-based laser-ranging technique commonly used for acquiring high-resolution topographic data.</skos:scopeNote>
    <!--FOR LIDAR images USE LIDAR WITH image collections-->
    <skos:altLabel xml:lang="en">LIDAR images</skos:altLabel>
    <skos:altLabel xml:lang="en">LIght Detection and Ranging</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/aerial%20photography">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">aerial photography</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The process of taking pictures with a camera from an aircraft. Use for both the process of photography from the air and the images produced by the process.</skos:scopeNote>
    <!--FOR aerial photographs USE aerial photography WITH image collections-->
    <skos:altLabel xml:lang="en">aerial photographs</skos:altLabel>
    <!--FOR aerial photos USE aerial photography WITH image collections-->
    <skos:altLabel xml:lang="en">aerial photos</skos:altLabel>
    <!--FOR air photos USE aerial photography WITH image collections-->
    <skos:altLabel xml:lang="en">air photos</skos:altLabel>
    <!--FOR orthoimagery USE aerial photography WITH image collections-->
    <skos:altLabel xml:lang="en">orthoimagery</skos:altLabel>
    <!--FOR orthophotographs USE aerial photography WITH image collections-->
    <skos:altLabel xml:lang="en">orthophotographs</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/photography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/aeromagnetic%20surveying">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">aeromagnetic surveying</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Mapping of the earth's magnetic field with the use of airborne electronic magnetometers.</skos:scopeNote>
    <!--FOR aeromagnetic maps USE aeromagnetic surveying WITH maps and atlases-->
    <skos:altLabel xml:lang="en">aeromagnetic maps</skos:altLabel>
    <!--FOR magnetic surveying USE aeromagnetic surveying WITH electromagnetic surveying-->
    <skos:altLabel xml:lang="en">magnetic surveying</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/electromagnetic%20surveying"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/aeroradiometric%20surveying">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">aeroradiometric surveying</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Mapping of gamma radiation from the earth's surface with airborne scintillation meters or gamma-ray spectrometers.</skos:scopeNote>
    <skos:altLabel xml:lang="en">gamma-ray spectrometric surveying</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/hyperspectral%20imaging">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">hyperspectral imaging</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Multispectral imaging technique that records many bands of imagery at very narrow bandwidths.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/infrared%20imaging">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">infrared imaging</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Method of remote sensing in which optical sensors produce visible representations of infrared rays or radiated heat from the observed objects and the temperature variations are represented by different colors in the image.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/AVHRR"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/AVIRIS"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/AVHRR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">AVHRR</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Advanced Very High Resolution Radiometer or broad-band, four or five channel scanner, sensing in the visible, near-infrared, and thermal infrared portions of the electromagnetic spectrum. This information is used to estimate characteristics of the land and sea surface such as temperature and overall health of vegetation.</skos:scopeNote>
    <!--FOR AVHRR images USE AVHRR WITH image collections-->
    <skos:altLabel xml:lang="en">AVHRR images</skos:altLabel>
    <skos:altLabel xml:lang="en">Advanced Very High Resolution Radiometer (AVHRR)</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/infrared%20imaging"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/sea%20surface%20temperature"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/AVIRIS">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">AVIRIS</skos:prefLabel>
    <skos:scopeNote xml:lang="en"></skos:scopeNote>
    <skos:altLabel xml:lang="en">airborne visible infrared imaging spectrometer</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/infrared%20imaging"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/microwave%20imaging">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">microwave imaging</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Remote sensing method using short high frequency electromagnetic waves reflected or radiated from the ground.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/SMMR"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/SSM%2FI"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/radar%20imaging"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/SMMR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">SMMR</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Scanning Multichannel Microwave Radiometer (SMMR), a technique which uses a multichannel microwave radiometer mounted on an airplane or satellite to measure polarized microwave radiances from the atmosphere, seas or land. Temperatures, wind speeds, sea ice coverage, and terrain parameters can be obtained.</skos:scopeNote>
    <skos:altLabel xml:lang="en">SMMR images</skos:altLabel>
    <skos:altLabel xml:lang="en">Scanning Multichannel Microwave Radiometer (SMMR)</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/microwave%20imaging"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/SSM%2FI">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">SSM/I</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Special Sensor Microwave/Imager (SSM/I), technique using passive microwave radiometers aboard satellites to measure the microwave reflectivity of the earth, oceans, and the atmosphere.</skos:scopeNote>
    <skos:altLabel xml:lang="en">SSM/I images</skos:altLabel>
    <skos:altLabel xml:lang="en">Special Sensor Microwave/Imager (SSM/I)</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/microwave%20imaging"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/snow%20and%20ice%20cover"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/multispectral%20imaging">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">multispectral imaging</skos:prefLabel>
    <skos:scopeNote xml:lang="en">A method of remote sensing that obtains optical representations in two or more ranges of frequencies or wave lengths.</skos:scopeNote>
    <skos:altLabel xml:lang="en">advanced spaceborne thermal emission and reflection radiometer</skos:altLabel>
    <skos:altLabel xml:lang="en">ASTER</skos:altLabel>
    <!--FOR Landsat images USE multispectral imaging WITH image collections-->
    <skos:altLabel xml:lang="en">Landsat images</skos:altLabel>
    <skos:altLabel xml:lang="en">thematic mapper</skos:altLabel>
    <skos:altLabel xml:lang="en">MODIS</skos:altLabel>
    <skos:altLabel xml:lang="en">Moderate-resolution imaging spectroradiometer</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/panchromatic%20imaging">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">panchromatic imaging</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Photographic technique which uses emulsions, films, or plates sensitive to all colors in light to produce black-and-white photographs. Often used in aerial and satellite photography.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/radar%20imaging">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">radar imaging</skos:prefLabel>
    <skos:scopeNote xml:lang="en">A remote sensing technique which uses the reflection of pulsed high frequency radio waves to determine the speed, direction, and distance of faraway objects.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/remote%20sensing"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/SLAR"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/doppler%20radar%20imaging"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geography"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/microwave%20imaging"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/SLAR">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">SLAR</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Side-looking Airborne Radar, an airborne radar imaging technique, uses an antenna mounted on an airplane or satellite to project radiation at right angles to the flight path and collect high resolution images.</skos:scopeNote>
    <skos:altLabel xml:lang="en">Side-Looking Airborne Radar (SLAR)</skos:altLabel>
