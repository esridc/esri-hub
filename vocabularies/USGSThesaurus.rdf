  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/parasites">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">parasites</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Symbiotic organisms that are detrimental to the host.</skos:scopeNote>
