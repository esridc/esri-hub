  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/overfishing">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">overfishing</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Taking too many fish from an area beyond the capacity for the population to replenish its numbers. The balance of the ecosystem is upset, leading to long-term depletion of fish stock.</skos:scopeNote>
    <skos:altLabel xml:lang="en">over-fishing</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/human%20impacts"/>
