    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">ecosystem diversity</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Measure of the variety of biological communities and habitats important for ecological stability.</skos:scopeNote>
