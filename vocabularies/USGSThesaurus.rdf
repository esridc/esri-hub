  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/well%20drilling">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">well drilling</skos:prefLabel>
    <skos:scopeNote xml:lang="en">The process of artificial excavation for the purpose of withdrawing water, oil, or gas from underground.</skos:scopeNote>
