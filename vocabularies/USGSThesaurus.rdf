    <skos:altLabel xml:lang="en">marine sediment transport</skos:altLabel>
    <!--FOR longshore drift USE sediment transport WITH longshore currents-->
    <skos:altLabel xml:lang="en">longshore drift</skos:altLabel>
    <!--FOR littoral drift USE sediment transport WITH longshore currents-->
    <skos:altLabel xml:lang="en">littoral drift</skos:altLabel>
    <!--FOR estuarine sediment transport USE sediment transport WITH estuarine processes-->
    <skos:altLabel xml:lang="en">estuarine sediment transport</skos:altLabel>
