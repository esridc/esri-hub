  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/heat%20flow%20%28earth%29">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">heat flow (earth)</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Energy transferred by a difference in temperature through the surface of the earth.</skos:scopeNote>
    <skos:altLabel xml:lang="en">geothermics</skos:altLabel>
    <skos:altLabel xml:lang="en">heatflow (earth)</skos:altLabel>
