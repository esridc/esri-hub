    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/biodegradation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">biodegradation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Reduction in the concentration of harmful chemicals by the action of animals and plants.</skos:scopeNote>
    <skos:altLabel xml:lang="en">natural attenuation (harmful chemicals)</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geochemical%20processes"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/carbon%20sequestration">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">carbon sequestration</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Long-term storage of carbon in soils and terrestrial organic material, typically of importance in understanding the balance of CO2 in the global climate system.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geochemical%20processes"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/ocean%20acidification">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">ocean acidification</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Decrease in the pH of ocean waters as a result of the increase in atmospheric carbon dioxide concentration.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geochemical%20processes"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/marine%20chemistry"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/effects%20of%20climate%20change"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/ocean%20processes"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/ore%20formation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">ore formation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Geochemical processes resulting in concentration of useful chemical elements and compounds, in earth materials.  Typically refers to metal ores.</skos:scopeNote>
    <skos:altLabel xml:lang="en">metallogeny</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geochemical%20processes"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/oxidation%20and%20reduction">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">oxidation and reduction</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Addition or reduction of oxygen in a compound accompanied by exchange of electrons among atoms.</skos:scopeNote>
    <skos:altLabel xml:lang="en">fermentation</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geochemical%20processes"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/reaction%20transport">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">reaction transport</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Chemical reactions occurring during ground-water flow.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geochemical%20processes"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/groundwater%20flow"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/sorption">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">sorption</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Attachment of chemical substances to materials, either externally (by adsorption) or internally (by absorption).</skos:scopeNote>
    <skos:altLabel xml:lang="en">adsorption</skos:altLabel>
    <skos:altLabel xml:lang="en">absorption</skos:altLabel>
