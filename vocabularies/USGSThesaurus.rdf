  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/enhanced%20oil%20recovery">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">enhanced oil recovery</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Injection of steam, gas, or other chemical compounds into hydrocarbon reservoirs to stimulate the production of usable oil beyond what is possible through natural pressure, water injection, and pumping at the wellhead.</skos:scopeNote>
    <skos:altLabel xml:lang="en">EOR</skos:altLabel>
    <skos:altLabel xml:lang="en">tertiary oil recovery</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20extraction"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/hydraulic%20fracturing">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">hydraulic fracturing</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Injection of water and other fluids through a well into geologic formations in order to increase the permeability of the rock units and thus permit natural gas or oil to be produced from them.</skos:scopeNote>
    <skos:altLabel xml:lang="en">hydrofracking</skos:altLabel>
    <skos:altLabel xml:lang="en">fracking</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/natural%20resource%20extraction"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
