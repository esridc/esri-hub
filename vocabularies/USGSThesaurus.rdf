  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/sinkhole%20formation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">sinkhole formation</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Localized collapse of the land surface usually due to erosion of the underlying materials.  Most commonly occurring in karst areas, but similar effects occur due to physical erosion in other terranes.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/erosion"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/karst"/>
