    <skos:prefLabel xml:lang="en">plankton</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Floating aquatic plants (phytoplankton) and animals (zooplankton) which are often microscopic and drift with the current in lakes, rivers, and oceans.</skos:scopeNote>
    <!--FOR planktonic ecosystems USE plankton WITH aquatic ecosystems-->
    <skos:altLabel xml:lang="en">planktonic ecosystems</skos:altLabel>
    <skos:altLabel xml:lang="en">zooplankton</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/organism%20groupings%20%28non-taxonomic%29"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/phytoplankton"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/aquatic%20biology"/>
