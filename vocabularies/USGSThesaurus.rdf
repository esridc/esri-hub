    <skos:altLabel xml:lang="en">soil creep</skos:altLabel>
    <skos:altLabel xml:lang="en">mass wasting</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geologic%20processes"/>
    <skos:narrower rdf:resource="http://www.usgs.gov/science/USGSThesaurus/landslides"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
