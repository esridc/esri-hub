  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/diagenesis">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">diagenesis</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Chemical, physical, and biological changes in sediment during and after its conversion to rock (lithification).</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geologic%20processes"/>
