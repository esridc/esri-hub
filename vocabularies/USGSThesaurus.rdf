    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/water%20chemistry"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/sound%20velocity">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">sound velocity</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Estimates of the velocity of sound in water, indexed by depth at a specific location, usually calculated from other water properties.</skos:scopeNote>
    <!--FOR sound velocity profiles USE sound velocity WITH profiles-->
    <skos:altLabel xml:lang="en">sound velocity profiles</skos:altLabel>
