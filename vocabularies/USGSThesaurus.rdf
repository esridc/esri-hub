    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/human%20impacts"/>
    <skos:inScheme rdf:resource="http://www.usgs.gov/science/USGSThesaurus"/>
  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/geochemical%20processes">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">geochemical processes</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Processes affecting the amount, distribution, or structure of chemical elements in air, water, soil, rocks, and minerals.</skos:scopeNote>
