  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/earthquakes">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">earthquakes</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Ground shaking caused by the sudden release of accumulated strain by an abrupt shift of rock along a fracture in the earth or by volcanic or magmatic activity, or other sudden stress changes in the earth.</skos:scopeNote>
    <!--FOR earthquake hazards USE earthquakes WITH hazards-->
    <skos:altLabel xml:lang="en">earthquake hazards</skos:altLabel>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/geologic%20processes"/>
