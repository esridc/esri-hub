    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">earthquake occurrences</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Time, location, severity, and mechanism of earthquake events, including the frequency and history of events in a given area.</skos:scopeNote>
    <!--FOR earthquake occurrence maps USE earthquake occurrences WITH maps and atlases-->
    <skos:altLabel xml:lang="en">earthquake occurrence maps</skos:altLabel>
