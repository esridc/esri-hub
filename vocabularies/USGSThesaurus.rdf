  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/nuisance%20species">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">nuisance species</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Organisms whose presence or behavior has undesirable effects on human activities.</skos:scopeNote>
