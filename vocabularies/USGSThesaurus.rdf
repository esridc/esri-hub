    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">pollinators</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Organisms that aid in the growth and distribution of plants by transferring pollen as a byproduct of their feeding activities.</skos:scopeNote>
