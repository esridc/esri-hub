  </rdf:Description>
  <rdf:Description about="http://www.usgs.gov/science/USGSThesaurus/phytoplankton">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">phytoplankton</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Small, mostly microscopic aquatic plants such as algae and bacteria suspended in water.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/plankton"/>
    <skos:related rdf:resource="http://www.usgs.gov/science/USGSThesaurus/calcareous%20nannoplankton"/>
