    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">mining hazards</skos:prefLabel>
    <skos:scopeNote xml:lang="en">Dangerous conditions which result from the extraction of naturally occurring mineral deposits.  Includes tunnel collapses, fires, and explosions.</skos:scopeNote>
    <skos:broader rdf:resource="http://www.usgs.gov/science/USGSThesaurus/human%20impacts"/>
